{
    "name": "valerio/extended-slim-php",
    "description": "A personal version of SlimPHP with some feature does not included in the vanilla version, like validation of the requestes.",
    "type": "project",
    "require": {
        "slim/slim-skeleton": "^3.1",
        "dusank/knapsack": "^10.0",
        "nesbot/carbon": "^1.32",
        "firebase/php-jwt": "^5.0",
        "gabordemooij/redbean": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\Components\\": "src/Components",
            "App\\Components\\Validation\\": "src/Components/Validation",
            "App\\Components\\Validation\\Exceptions\\": "src/Components/Validation/Exceptions",
            "App\\Components\\Validation\\Interfaces\\": "src/Components/Validation/Interfaces",
            "App\\Components\\Validation\\Rules\\": "src/Components/Validation/Rules",
            "App\\Middlewares\\": "src/Middlewares",
            "App\\Controllers\\": "src/Controllers",
            "App\\Utils\\": "src/Utils"
        }
    },
    "license": "MIT",
